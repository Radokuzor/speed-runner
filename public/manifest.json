{
    "manifest_version": 3,
    "name": "Copilot Realtor",
    "description": "Your 4.0 Solution",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "4.0 hero"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "scripting",
        
        "storage",
        "contextMenus"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
}